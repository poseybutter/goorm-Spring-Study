# MySQL Database Configuration
# Root 사용자 비밀번호
MYSQL_ROOT_PASSWORD=your_root_password_here

# 데이터베이스 이름
MYSQL_DATABASE=diary_db

# 데이터베이스 사용자 이름
MYSQL_USER=your_db_user_here

# 데이터베이스 사용자 비밀번호
MYSQL_PASSWORD=your_db_password_here

# MySQL Connection Info for Spring Boot
# 데이터베이스 호스트 (Docker 사용시 localhost)
DB_HOST=localhost

# 데이터베이스 포트
DB_PORT=3306

# 데이터베이스 이름
DB_NAME=diary_db

# Spring Boot 애플리케이션에서 사용할 DB 사용자명
DB_USERNAME=root_or_your_username

# Spring Boot 애플리케이션에서 사용할 DB 비밀번호
DB_PASSWORD=your_password_here

# Application Configuration
# 애플리케이션 포트
APP_PORT=8080

# 타임존 설정
TZ=Asia/Seoul